<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="description" content="Personal website of {{ constants.firstName }} {{ constants.lastName }}, IT guy and Engineer.">
<meta name="viewport" content="width=device-width, initial-scale=1">

<meta name="msapplication-TileColor" content="{{ buildConstants.msTileColor }}">
<meta name="theme-color" content="{{ buildConstants.themeColor }}">

<link rel="apple-touch-icon" sizes="180x180" href="{{ buildConstants.appleIconPrefix }}{{ buildConstants.pngExtension }}?v={{ buildConstants.siteVersion }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ buildConstants.faviconPrefix }}-32x32{{ buildConstants.pngExtension }}?v={{ buildConstants.siteVersion }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ buildConstants.faviconPrefix }}-16x16{{ buildConstants.pngExtension }}?v={{ buildConstants.siteVersion }}">
<link rel="manifest" href="{{ buildConstants.webManifestFileName }}?v={{ buildConstants.siteVersion }}">
<link rel="mask-icon" href="{{ buildConstants.safariIconFileName }}?v={{ buildConstants.siteVersion }}" color="{{ buildConstants.themeColor }}">
<link rel="shortcut icon" href="{{ buildConstants.faviconPrefix }}{{ buildConstants.icoExtension }}?v={{ buildConstants.siteVersion }}">

<link rel="preload" as="font" type="font/woff2" href="fonts/{{ buildConstants.fontFileName }}?v={{ buildConstants.siteVersion }}" crossorigin>
<link rel="stylesheet" href="css/main{{ buildConstants.cssExtension }}?v={{ buildConstants.siteVersion }}">

<link rel="canonical" href="{{ constants.baseUrl }}">
