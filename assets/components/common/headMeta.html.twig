<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="description" content="Personal website of {{ constants.firstName }} {{ constants.lastName }}, IT guy and Engineer.">
<meta name="viewport" content="width=device-width, initial-scale=1">

<meta name="msapplication-TileColor" content="{{ constants.msTileColor }}">
<meta name="theme-color" content="{{ constants.themeColor }}">

<link rel="apple-touch-icon" sizes="180x180" href="{{ constants.appleIconPrefix }}.png?v={{ constants.siteVersion }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ constants.faviconPrefix }}-32x32.png?v={{ constants.siteVersion }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ constants.faviconPrefix }}-16x16.png?v={{ constants.siteVersion }}">
<link rel="manifest" href="{{ constants.webManifestFileName }}?v={{ constants.siteVersion }}">
<link rel="mask-icon" href="{{ constants.safariIconFileName }}?v={{ constants.siteVersion }}" color="{{ constants.themeColor }}">
<link rel="shortcut icon" href="{{ constants.faviconPrefix }}.ico?v={{ constants.siteVersion }}">

<link rel="preload" as="font" type="font/woff2" href="fonts/{{ constants.fontFileName }}?v={{ constants.siteVersion }}" crossorigin>
<link rel="stylesheet" href="css/{{ constants.outputCssFileName }}?v={{ constants.siteVersion }}">

<link rel="canonical" href="{{ constants.baseUrl }}">
