            <square144x144logo src="/{{ buildConstants.msTileIconPrefix }}-144x144{{ buildConstants.pngExtension }}?v={{ constants.siteVersion}}"/>
            <square150x150logo src="/{{ buildConstants.msTileIconPrefix }}-150x150{{ buildConstants.pngExtension }}?v={{ constants.siteVersion}}"/>
