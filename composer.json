{
    "name": "birkett/a-birkett-co-uk",
    "license": "MIT",
    "type": "project",
    "description": "Personal website of Anthony Birkett",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^2.0@dev",
        "sensio/framework-extra-bundle": "^5.4",
        "sensiolabs/security-checker": "^6.0",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/debug": "^4.3",
        "symfony/debug-bundle": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/monolog-bundle": "^3.4",
        "symfony/security": "^4.3",
        "symfony/security-csrf": "^4.3",
        "symfony/twig-bridge": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/web-profiler-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "twig/twig": "^2.11",
        "twitter/bootstrap": "^4.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.12",
        "phpunit/php-code-coverage": "^7.0",
        "phpunit/phpunit": "^8.2",
        "sebastian/global-state": "^3.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/browser-kit": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/phpunit-bridge": "^4.3"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "id": "01CBFF3EZSWGCWP60030R9G8G8",
            "allow-contrib": false
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
